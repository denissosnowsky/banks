{
  "dependencies": {
    "concurrently": "^6.2.1"
  },
  "scripts": {
    "start:client": "cd client && yarn start",
    "start:server": "cd server && yarn run dev",
    "install:client": "cd client && yarn install",
    "install:server": "cd server && yarn install",
    "migrate": "cd server && yarn run first_migrate",
